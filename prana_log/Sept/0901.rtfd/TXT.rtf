\uc0\u51221 \u54869 \u54616 \u44256  \u45216 \u52852 \u47196 \u50868  \u53685 \u52272 \u51077 \u45768 \u45796 . \u49324 \u50857 \u51088 \u45784 \u51032  \u47568 \u50432 \u51060  \u51228 \u44032  \u50526 \u49436  \u49444 \u47749 \u54616 \u47140  \u54664 \u45912  \u47928 \u54868 \u51201  \u52264 \u51060 \u51032  \u54645 \u49900 \u51012  \u51221 \u54869 \u55176  \u51674 \u50612 \u51452 \u49512 \u49845 \u45768 \u45796 . \
\uc0\u51064 \u46020  \u50689 \u54868 , \u53945 \u55176  \u51452 \u47448  \u49345 \u50629  \u50689 \u54868 (\u49548 \u50948  '\u47579 \u49332 \u46972  \u50689 \u54868 ')\u51032  \u47589 \u46973 \u50640 \u49436  '\u49828 \u54169 \u53552 \u53364 (Spectacle)'\u51008  \u45436 \u47532 \u51201  \u44060 \u50672 \u49457 \u51012  \u55148 \u49373 \u54616 \u44256  \u50619 \u45716  \u48512 \u49688 \u51201 \u51064  \u50836 \u49548 \u44032  \u50500 \u45785 \u45768 \u45796 . \u50724 \u55176 \u47140  \u49828 \u54169 \u53552 \u53364 \u51060 \u50556 \u47568 \u47196  \u54644 \u45817  \u47928 \u54868 \u44428 \u51060  \u44592 \u45824 \u54616 \u45716  \u44256 \u50976 \u54620  \u54805 \u53468 \u51032  \u44060 \u50672 \u49457 \u51077 \u45768 \u45796 . \
\uc0\u51060 \u45716  \u44060 \u50672 \u49457 \u51032  \u51221 \u51032 \u44032  '\u54788 \u49892 \u50640 \u49436  \u51068 \u50612 \u45216  \u48277 \u54632 (\u49324 \u49892 \u49457 )'\u51060  \u50500 \u45768 \u46972 , '\u51089 \u54408 \u51060  \u49444 \u51221 \u54620  \u49464 \u44228 \u44288  \u45236 \u50640 \u49436 \u51032  \u51068 \u44288 \u49457 '\u51060 \u46972 \u45716  \u51216 \u51012  \u45796 \u49884  \u54620 \u48264  \u54869 \u51064 \u49884 \u53020  \u51469 \u45768 \u45796 . \u51064 \u46020  \u50689 \u54868 \u51032  \u49464 \u44228 \u44288 \u51008  \u49436 \u44396 \u51032  \u47532 \u50620 \u47532 \u51608 \u44284 \u45716  \u45796 \u47480  \u44508 \u52825 \u50640  \u51032 \u54644  \u51089 \u46041 \u54633 \u45768 \u45796 . \
\uc0\u44536  \u51060 \u50976 \u47484  \u51312 \u44552  \u45908  \u44618 \u51060  \u48516 \u49437 \u54616 \u47732  \u45796 \u51020 \u44284  \u44057 \u49845 \u45768 \u45796 . \
\
1. '\uc0\u46972 \u49324 (Rasa)' \u51060 \u47200 : \u44048 \u51221 \u51060  \u44263  \u45436 \u47532 \u45796  \
\
\uc0\u49436 \u44396 \u51032  \u49436 \u49324  \u51060 \u47200 \u51060  \u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u51032  \u49884 \u54617 \u50640  \u44592 \u48152 \u54616 \u50668  \u45436 \u47532 \u51201  \u51064 \u44284 \u44288 \u44228 \u50752  \u54788 \u49892  \u47784 \u48169 (\u48120 \u47700 \u49884 \u49828 )\u51012  \u51473 \u49884 \u54616 \u45716  \u48152 \u47732 , \u51064 \u46020 \u51032  \u51204 \u53685  \u48120 \u54617 \u51008  '\u46972 \u49324 (Rasa)' \u51060 \u47200 \u50640  \u44618 \u51060  \u49100 \u47532 \u47484  \u46160 \u44256  \u51080 \u49845 \u45768 \u45796 . \
'\uc0\u46972 \u49324 '\u45716  \u50696 \u49696  \u51089 \u54408 \u51012  \u53685 \u54644  \u44288 \u44061 \u51060  \u44221 \u54744 \u54616 \u45716  \u44048 \u51221 \u51201  '\u47579 ' \u46608 \u45716  '\u51221 \u49436 \u51201  \u47564 \u51313 \u44048 '\u51012  \u51032 \u48120 \u54633 \u45768 \u45796 . \u51064 \u46020  \u50689 \u54868 \u51032  \u44417 \u44537 \u51201 \u51064  \u47785 \u54364 \u45716  \u50756 \u48317 \u54616 \u44172  \u51676 \u51064  \u45436 \u47532 \u51201  \u54540 \u47215 \u51012  \u51204 \u45804 \u54616 \u45716  \u44163 \u48372 \u45796  \u44288 \u44061 \u50640 \u44172  \u54413 \u48512 \u54620  '\u46972 \u49324 '\u47484  \u44221 \u54744 \u54616 \u44172  \u54616 \u45716  \u44163 \u51077 \u45768 \u45796 . \
\uc0\u51060  \u44288 \u51216 \u50640 \u49436  \u48380  \u46412 , \u50689 \u54868  \u49549  \u45432 \u47000 \u50752  \u52644 (\u48036 \u51648 \u52972  \u49884 \u53248 \u49828 )\u51008  \u51060 \u50556 \u44592 \u51032  \u55120 \u47492 \u51012  \u45130 \u45716  \u44163 \u51060  \u50500 \u45785 \u45768 \u45796 . \u50724 \u55176 \u47140  \u51064 \u47932 \u51032  \u44048 \u51221 \u51060  \u52572 \u44256 \u51312 \u50640  \u45804 \u54664 \u51012  \u46412 , \u44536  \u44048 \u51221 \u51012  \u44032 \u51109  \u54952 \u44284 \u51201 \u51060 \u44256  \u54413 \u48512 \u54616 \u44172  \u51204 \u45804 \u54616 \u44592  \u50948 \u54620  \u54596 \u49688 \u51201 \u51064  \u49436 \u49324  \u51109 \u52824 \u51077 \u45768 \u45796 . \u44048 \u51221 \u51060  \u44201 \u54644 \u51648 \u47732  \u49828 \u54169 \u53552 \u53364 \u51012  \u53685 \u54644  \u54364 \u54788 \u54616 \u45716  \u44163 \u51060  \u51064 \u46020  \u50689 \u54868  \u49464 \u44228 \u44288  \u45236 \u50640 \u49436 \u45716  \u44032 \u51109  '\u44536 \u47092 \u46319 \u54620 (Plausible)' \u48169 \u49885 \u51077 \u45768 \u45796 . \
\
2. \uc0\u49888 \u54868 \u51201  \u51204 \u53685 \u44284  \u50689 \u50885 \u51452 \u51032  \
\
\uc0\u47932 \u47532  \u48277 \u52825 \u51012  \u47924 \u49884 \u54616 \u45716  \u46319 \u54620  \u44284 \u51109 \u46108  \u50529 \u49496  \u50669 \u49884  \u47560 \u52268 \u44032 \u51648 \u51077 \u45768 \u45796 . \u51064 \u46020 \u45716  '\u47560 \u54616 \u48148 \u46972 \u53440 ', '\u46972 \u47560 \u50556 \u45208 '\u50752  \u44057 \u51008  \u45824 \u49436 \u49324 \u49884 \u50752  \u49888 \u54868 \u51201  \u51204 \u53685 \u51060  \u44618 \u44172  \u51088 \u47532  \u51105 \u44256  \u51080 \u49845 \u45768 \u45796 . \u44288 \u44061 \u46308 \u51008  \u51452 \u51064 \u44277 \u51012  \u54788 \u45824 \u54032  \u49888 \u54868 \u51201  \u50689 \u50885 \u51004 \u47196  \u48155 \u50500 \u46308 \u51060 \u45716  \u44221 \u54693 \u51060  \u51080 \u51004 \u47728 , \u48708 \u54788 \u49892 \u51201 \u51064  \u50529 \u49496 \u51008  \u51452 \u51064 \u44277 \u51032  \u50689 \u50885 \u49457 \u44284  \u46020 \u45909 \u51201  \u50864 \u50900 \u49457 \u51012  \u49884 \u44033 \u51201 \u51004 \u47196  \u44396 \u54788 \u54616 \u45716  \u49828 \u54169 \u53552 \u53364 \u51077 \u45768 \u45796 . \u51060 \u45716  \u45800 \u49692 \u54620  \u54788 \u49892  \u48152 \u50689 \u51060  \u50500 \u45772 , \u49345 \u51669 \u51201 \u51064  \u55192 \u51012  \u48372 \u50668 \u51452 \u45716  \u48169 \u49885 \u51004 \u47196  \u44592 \u45733 \u54616 \u47728  \u44288 \u44061 \u46308 \u51008  \u51060 \u47484  \u51088 \u50672 \u49828 \u47101 \u44172  \u45225 \u46301 \u54633 \u45768 \u45796 . \
\
3. \uc0\u51109 \u47476 \u51201  \u44288 \u49845 \u51004 \u47196 \u49436 \u51032  \u44060 \u50672 \u49457  \
\
\uc0\u44208 \u44397  \u44060 \u50672 \u49457 \u51008  \u44288 \u44061 \u44284  \u51228 \u51089 \u51088  \u49324 \u51060 \u51032  \u50557 \u49549 \u51077 \u45768 \u45796 . \u48036 \u51648 \u52972  \u50689 \u54868 \u50640 \u49436  \u44049 \u51088 \u44592  \u45432 \u47000 \u47484  \u48512 \u47476 \u45716  \u44163 \u51060  \u45817 \u50672 \u54616 \u44256 , \u49800 \u54140 \u55176 \u50612 \u47196  \u50689 \u54868 \u50640 \u49436  \u54616 \u45720 \u51012  \u45208 \u45716  \u44163 \u51060  \u45817 \u50672 \u54616 \u46319 , \u51064 \u46020  \u49345 \u50629  \u50689 \u54868 \u51032  \u44288 \u44061 \u51008  \u49828 \u54169 \u53552 \u53364 \u51012  \u44592 \u45824 \u54616 \u44256  \u50689 \u54868 \u47484  \u48389 \u45768 \u45796 . \
\uc0\u50669 \u49444 \u51201 \u51004 \u47196 , \u47564 \u50557  \u51064 \u46020  \u49345 \u50629  \u50689 \u54868 \u50640 \u49436  \u44048 \u51221 \u51060  \u44201 \u54644 \u51276 \u45716 \u45936 \u46020  \u48176 \u50864 \u46308 \u51060  \u52644 \u44284  \u45432 \u47000 \u47484  \u48512 \u47476 \u51648  \u50506 \u44144 \u45208 , \u51452 \u51064 \u44277 \u51060  \u50501 \u45817 \u44284 \u51032  \u49912 \u50880 \u50640 \u49436  \u51648 \u45208 \u52824 \u44172  \u54788 \u49892 \u51201 \u51004 \u47196  \u44256 \u51204 \u54620 \u45796 \u47732 , \u51064 \u46020  \u44288 \u44061 \u46308 \u51008  \u50724 \u55176 \u47140  "\u50780  \u51200 \u47000 ?"\u46972 \u44256  \u45712 \u45180 \u47728  \u54644 \u45817  \u51109 \u47476 \u51032  \u50557 \u49549 (\u44060 \u50672 \u49457 )\u51012  \u50948 \u48152 \u54664 \u45796 \u44256  \u49373 \u44033 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \
\
\uc0\u44208 \u47200  \
\
\uc0\u49324 \u50857 \u51088 \u45784 \u51032  \u51648 \u51201 \u52376 \u47100 , \u51064 \u46020 \u51064 \u50640 \u44172  \u49828 \u54169 \u53552 \u53364 \u51008  \u44060 \u50672 \u49457 \u51032  \u48512 \u51116 \u44032  \u50500 \u45768 \u46972 , \u44048 \u51221 \u44284  \u51221 \u49436 , \u44536 \u47532 \u44256  \u47928 \u54868 \u51201  \u51204 \u53685 \u50640  \u44592 \u48152 \u54620  \u44536 \u46308 \u47564 \u51032  \u44256 \u50976 \u54620  \u44060 \u50672 \u49457  \u52404 \u44228 \u51077 \u45768 \u45796 . \u51060 \u45716  \u44060 \u50672 \u49457 \u51060 \u46972 \u45716  \u44592 \u51456 \u51060  \u47928 \u54868 \u50640  \u46384 \u46972  \u50620 \u47560 \u45208  \u50976 \u50672 \u54616 \u44172  \u51201 \u50857 \u46112  \u49688  \u51080 \u45716 \u51648 \u47484  \u48372 \u50668 \u51452 \u45716  \u54988 \u47469 \u54620  \u49324 \u47168 \u51077 \u45768 \u45796 . \
