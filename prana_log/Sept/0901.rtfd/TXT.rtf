\uc0\u49373 \u44033 \u54616 \u45716  \u44284 \u51221  \u54364 \u49884  \
\
\uc0\u47588 \u50864  \u50696 \u47532 \u54616 \u44256  \u52384 \u54617 \u51201 \u51064  \u51648 \u51201 \u51077 \u45768 \u45796 . \u51060 \u45716  \u45800 \u49692 \u55176  \u49436 \u49324  \u51060 \u47200 \u50640  \u45824 \u54620  \u45436 \u51032 \u47484  \u45336 \u50612 , \u50864 \u47532 \u44032  '\u51060 \u49457 '\u44284  '\u45436 \u47532 '\u47484  \u50612 \u46523 \u44172  \u51060 \u54644 \u54616 \u44256  \u54876 \u50857 \u54616 \u45716 \u51648 \u50640  \u45824 \u54620  \u44540 \u48376 \u51201 \u51064  \u49457 \u52272 \u51012  \u50836 \u44396 \u54633 \u45768 \u45796 . \
\uc0\u49324 \u50857 \u51088 \u45784 \u51032  \u47568 \u50432 \u51008 , \u51228 \u44032  \u51060 \u49457 \u51201  \u45436 \u47532 \u51032  \u51473 \u50836 \u49457 \u51012  \u51060 \u50556 \u44592 \u54616 \u47732 \u49436 \u46020 , \u51221 \u51089  \u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u44032  \u47932 \u47140 \u51456  \u50976 \u49328 \u51032  \u48376 \u51656 \u51012  \u45459 \u52824 \u44256  \u51080 \u44144 \u45208  \u54200 \u54801 \u54616 \u44172  \u49324 \u50857 \u54616 \u44256  \u51080 \u45716  \u44163 \u51008  \u50500 \u45772 \u51648  \u48708 \u54032 \u54616 \u49888  \u44163 \u51004 \u47196  \u51060 \u54644 \u46121 \u45768 \u45796 . \u44536 \u47532 \u44256  \u44536  \u48708 \u54032 \u51008  \u47588 \u50864  \u53440 \u45817 \u54633 \u45768 \u45796 . \
\uc0\u51228 \u44032  \u54788 \u49345 \u51012  \u48516 \u49437 \u54616 \u44256  \u49444 \u47749 \u54616 \u45716  \u48169 \u49885 \u51008  \u54596 \u50672 \u51201 \u51004 \u47196  \u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u44032  \u52404 \u44228 \u54868 \u54620  \u48516 \u49437 \u51201  \u49324 \u44256 \u50640  \u51032 \u51316 \u54633 \u45768 \u45796 . \u54616 \u51648 \u47564  '\u51060 \u49457 \u51032  \u45436 \u47532 '\u47484  \u44053 \u51312 \u54616 \u47732 \u49436  \u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u51032  \u49440 \u47932 \u51012  \u51228 \u45824 \u47196  \u49324 \u50857 \u54616 \u51648  \u47803 \u54616 \u44256  \u51080 \u45796 \u45716  \u51648 \u51201 \u51008  \u45796 \u51020 \u44284  \u44057 \u51008  \u51060 \u50976 \u50640 \u49436  \u51473 \u50836 \u54633 \u45768 \u45796 . \
\
1. \uc0\u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u51032  \u49440 \u47932 : \u47196 \u44256 \u49828 (Logos)\u47484  \u45336 \u50612 \u49436  \
\
\uc0\u50864 \u47532 \u45716  \u51333 \u51333  \u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u47484  '\u45436 \u47532 \u54617 \u51032  \u50500 \u48260 \u51648 '\u47196 \u47564  \u49373 \u44033 \u54616 \u51648 \u47564 , \u44536 \u51032  \u50976 \u49328 \u51008  \u55016 \u50476  \u45908  \u54413 \u48512 \u54633 \u45768 \u45796 . \u44536 \u45716  \u12302 \u49688 \u49324 \u54617 \u12303 \u44284  \u12302 \u49884 \u54617 \u12303 \u51012  \u53685 \u54644  \u51064 \u44036 \u44284  \u51060 \u50556 \u44592 \u50640  \u45824 \u54644  \u44618 \u51060  \u53456 \u44396 \u54664 \u49845 \u45768 \u45796 . \
\uc0\u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u45716  \u49444 \u46301 \u51032  \u49464  \u44032 \u51648  \u54645 \u49900  \u50836 \u49548 \u47484  \u51228 \u49884 \u54664 \u49845 \u45768 \u45796 . \
\
\uc0\u47196 \u44256 \u49828  (Logos): \u45436 \u47532 \u51201 \u51060 \u44256  \u51060 \u49457 \u51201 \u51064  \u52628 \u47200 . \
\uc0\u54028 \u53664 \u49828  (Pathos): \u52397 \u51473 \u51032  \u44048 \u51221 \u51201  \u48152 \u51025 \u44284  \u49900 \u47532  \u49345 \u53468 . \
\uc0\u50640 \u53664 \u49828  (Ethos): \u54868 \u51088 \u51032  \u49457 \u54408 , \u49888 \u47280 \u49457 , \u50980 \u47532 \u49457 (\u46608 \u45716  \u47928 \u54868 \u51201  \u47589 \u46973 ). \
\uc0\u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u45716  \u54988 \u47469 \u54620  \u49444 \u46301 \u51060 \u45208  \u51060 \u50556 \u44592 \u44032  \u51060  \u49464  \u44032 \u51648  \u50836 \u49548 \u47484  \u44512 \u54805  \u51080 \u44172  \u54596 \u50836 \u47196  \u54620 \u45796 \u44256  \u48372 \u50520 \u49845 \u45768 \u45796 . \u53945 \u55176  \u50696 \u49696 \u44284  \u49436 \u49324 \u50640 \u49436 \u45716  '\u54028 \u53664 \u49828 '\u51032  \u50669 \u54624 \u51060  \u44208 \u51221 \u51201 \u51077 \u45768 \u45796 . \
\uc0\u12302 \u49884 \u54617 \u12303 \u50640 \u49436  \u44536 \u44032  \u51228 \u49884 \u54620  \u48708 \u44537 \u51032  \u47785 \u54364 \u45716  \u45800 \u49692 \u55176  \u45436 \u47532 \u51201 \u51064  \u54540 \u47215 \u51012  \u51204 \u45804 \u54616 \u45716  \u44163 \u51060  \u50500 \u45785 \u45768 \u45796 . \u44288 \u44061 \u51060  \u50672 \u48124 \u44284  \u44277 \u54252 \u47484  \u45712 \u45180 \u44172  \u54616 \u50668  \u52852 \u53440 \u47476 \u49884 \u49828 (Catharsis), \u51593  \u44048 \u51221 \u51032  \u51221 \u54868 \u47484  \u44221 \u54744 \u54616 \u44172  \u54616 \u45716  \u44163 \u51077 \u45768 \u45796 . \u46384 \u46972 \u49436  \u50500 \u47532 \u49828 \u53664 \u53588 \u47112 \u49828 \u51032  \u44288 \u51216 \u50640 \u49436  \u49436 \u49324 \u51032  \u50756 \u49457 \u46020 \u45716  \u45436 \u47532 \u51201  \u53440 \u45817 \u49457 (\u47196 \u44256 \u49828 )\u49104 \u47564  \u50500 \u45768 \u46972  \u44048 \u51221 \u51201  \u54952 \u44284 (\u54028 \u53664 \u49828 )\u44620 \u51648  \u54252 \u54632 \u54616 \u45716  \u44060 \u45392 \u51077 \u45768 \u45796 . \
\
2. \uc0\u54788 \u45824 \u51032  \u50780 \u44257 \u46108  '\u51060 \u49457 '\u44284  '\u44060 \u50672 \u49457 ' \u51665 \u52265  \
